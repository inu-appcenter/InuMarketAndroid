<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="335dp"
    android:layout_height="253dp"
    android:background="@drawable/square_allradius">


    <TextView
        android:id="@+id/di_change_tv"
        android:layout_width="wrap_content"
        android:layout_height="29dp"
        android:textSize="20sp"
        android:text="TextView"
        android:layout_marginTop="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="68dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="4dp"
        android:textSize="20sp"
        android:text="판매 완료하시겠습니까?"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/di_change_tv" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="54dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="28dp"
        android:text="상품이 판매 완료되면 고객이\n 상세정보를 열람할 수 없게 됩니다."
        android:textSize="16sp"
        android:gravity="center_horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <View
        android:id="@+id/view8"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="33dp"
        android:background="@color/cloudy_blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <Button
        android:id="@+id/di_change_cancle"
        android:layout_width="168dp"
        android:layout_height="56dp"
        android:background="@android:color/transparent"
        android:text="취소"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <View
        android:id="@+id/view9"
        android:layout_width="1dp"
        android:layout_height="56dp"
        android:background="@color/cloudy_blue"
        app:layout_constraintEnd_toStartOf="@+id/di_change_submit"
        app:layout_constraintStart_toEndOf="@+id/di_change_cancle"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <Button
        android:id="@+id/di_change_submit"
        android:layout_width="168dp"
        android:layout_height="56dp"
        android:text="판매완료"
        android:textSize="16sp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

</android.support.constraint.ConstraintLayout>